#derive[match]
type Maybe (a: Type) {
    some (val: a)
    none
}

Str : Type
Str.nil : Str

MotiveGen (n: Maybe U60) : Type
MotiveGen (Maybe.some _) = U60
MotiveGen Maybe.none     = Str

Main : U60
Main =
    match Maybe (Maybe.some 2) {
        some val => 2
        none     => Str.nil
    } : (x => MotiveGen x)